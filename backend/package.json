{
  "name": "mindquest-backend",
  "version": "1.0.0",
  "description": "Backend for MindQuest application",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "start": "cls && node src/app.js",
    "dev": "cls && nodemon src/app.js",
    "test": "cls && node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "cls && node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "cls && node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:ci": "cls && node --experimental-vm-modules node_modules/jest/bin/jest.js --ci --coverage --runInBand",
    "setup:firebase": "node scripts/setup-firebase.js",
    "seed:data": "node scripts/seed-data.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@arcjet/node": "1.0.0-beta.6",
    "@dotenvx/dotenvx": "^0.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "firebase": "^11.7.1",
    "firebase-admin": "^13.4.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^29.7.0",
    "chalk": "^5.4.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.3"
  }
}
